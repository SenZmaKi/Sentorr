{
  "name": "Sentorr",
  "version": "1.0.0",
  "description": "Stream Movies and TV Shows from Torrents",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "rollup-build": "rollup -c --bundleConfigAsCjs",
    "rollup-dev": "rollup -c -w --bundleConfigAsCjs",
    "start": "sirv public --no-clear",
    "svelte-electron": "wait-on http://localhost:8080 && electron .",
    "dev": "concurrently \"rollup-dev\" \"tsc -w\" \"svelte-electron\"",
    "preelectron-pack": "npm run rollup-build",
    "elecmron-pack": "electron-builder"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": ["node_modules/", "tests/utils/*"],
    "testMatch": ["**/tests/**/*.[jt]s?(x)", "**/?(*.)+(spec|test).[tj]s?(x)"],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "testTimeout": 69000
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ctrl/video-filename-parser": "^5.1.0",
    "cheerio": "^1.0.0-rc.12",
    "electron-serve": "^1.2.0",
    "fs": "^0.0.1-security",
    "htmlparser2": "^9.0.0",
    "sirv-cli": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.9.4",
    "@types/torrent-search-api": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "electron": "^27.1.2",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "rollup": "^3.15.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.2",
    "svelte": "^4.2.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "wait-on": "^6.0.1"
  },
  "moduleDirectories": {
    "node_modules": [
      "node_modules",
      "src"
    ]
  }
}
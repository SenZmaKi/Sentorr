{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/**/*",
    "test/**/*",
    "@types/**/*",
  ],
  "compilerOptions": {
    "composite": true,
    "types": [
      "electron-vite/node",
      "bun"
    ],
    "paths": {
      "@/*": [
        "./src/*",
      ],
      "@/test/*": [
        "./test/*"
      ]
    }
  }
}